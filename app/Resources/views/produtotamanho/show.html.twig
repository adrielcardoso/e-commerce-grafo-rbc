{% extends 'base.html.twig' %}

{% block body %}
    <h1>ProdutoTamanho</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ produtoTamanho.id }}</td>
            </tr>
            <tr>
                <th>Descricao</th>
                <td>{{ produtoTamanho.descricao }}</td>
            </tr>
            <tr>
                <th>Peso</th>
                <td>{{ produtoTamanho.peso }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('produtotamanho_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('produtotamanho_edit', { 'id': produtoTamanho.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
